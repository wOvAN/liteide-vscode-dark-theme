<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="VSCode">
  <style name="Text" foreground="#D4D4D4" background="#1e1e1e"/>
  <style name="Extra" foreground="#858585" background="#1e1e1e"/>
  <style name="Selection" background="#264F78"/>   
  <style name="CurrentLine" background="#282828"/><!-- Current line where the cursor is -->
  <style name="MatchBrackets" background="#888888"/>
  <style name="IndentLine" foreground="#404040"/>   <!-- 80 pixel margins -->
  <style name="VisualWhitespace" foreground="#c0c0c0"/> <!-- Space after code -->
  <style name="Keyword" foreground="#569CD6"/><!-- Keywords: package, import, func, return, if, type, interface --><!-- Also controls the command at the top of the Build Output window -->
  <style name="DataType" foreground="#9CDCFE" bold="false" />
  <style name="Decimal" foreground="#B5CEA8" bold="false"/>
  <style name="BaseN" foreground="#6d9cbe"/>
  <style name="Float" foreground="#6d9cbe"/>
  <style name="Char" foreground="#D7BA7D"/>
  <style name="String" foreground="#ce9178"/>
  <style name="Comment" foreground="#6a9955"/>
  <style name="Alert" foreground="#ff0000"/>
  <style name="Error" foreground="#ff0000"/>
  <style name="Function" foreground="#DCDCAA"/>
  <style name="RegionMarker" foreground="#FFD700"/>
  <style name="Symbol" foreground="#bbbbbb"/>
  <style name="BuiltinFunc" foreground="#dcdcaa"/>
  <style name="Predeclared" foreground="#C586C0"/>
  <style name="FuncDecl" foreground="#dcdcaa"/>
  <style name="Placeholder" foreground="#c261c2"/>
  <style name="ToDo" foreground="#6a9955" background="#FFBD2A" />
</style-scheme>
